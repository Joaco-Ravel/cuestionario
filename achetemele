<div id="resultado" class="seccion">

<h3>Tabla de Resultados (Acumulados por grupo)</h3>
<table id="tablaResultados">
    <thead>
        <tr>
            <th>Grupo</th>
            <th>Suma de Valores</th>
        </tr>
    </thead>
    <tbody></tbody>
</table>

<h3>Gráfica de Barras</h3>
<canvas id="graficaResultados" width="400" height="200"></canvas>
</div>


<script>
   function mostrarSeccion(id) {
      // Oculta todas las secciones
      var secciones = document.querySelectorAll('.seccion');
      secciones.forEach(function(seccion) {
        seccion.classList.remove('activa');
      });

      // Muestra la sección seleccionada
      var seccionActiva = document.getElementById(id);
      seccionActiva.classList.add('activa');
    }

    function mostrarResultados() {
        const respuestas = [];
        for (let i = 1; i <= 60; i++) {
            const valor = parseInt(document.querySelector(`[name=pregunta${i}]`).value);
            respuestas.push(valor);
        }

        const sumaPorGrupo = Array(10).fill(0);
        for (let i = 0; i < 60; i++) {
            const indiceGrupo = i % 10;
            sumaPorGrupo[indiceGrupo] += respuestas[i];
        }

        const tbody = document.querySelector('#tablaResultados tbody');
        tbody.innerHTML = '';
	labels= ['Servicio Social','Ejecutivo Persuasivo','Verbal','Artístico Plástico','Artístico Musical','Organización','Científico','Cálculo','Mecánico Constructivo','Actividad al Aire Libre'],
        sumaPorGrupo.forEach((valor, index) => {
            const row = `<tr><td>${labels[index]}</td><td>${valor}</td></tr>`;
            tbody.innerHTML += row;
        });

        const ctx = document.getElementById('graficaResultados').getContext('2d');
        if (window.miGrafica) {
            window.miGrafica.destroy();
        }
        window.miGrafica = new Chart(ctx, {
            type: 'bar',
            data: {
                labels: ['Servicio Social','Ejecutivo Persuasivo','Verbal','Artístico Plástico','Artístico Musical','Organización','Científico','Cálculo','Mecánico Constructivo','Actividad al Aire Libre'],
                datasets: [{
                    label: 'Suma de Respuestas',
                    data: sumaPorGrupo,
                    backgroundColor: 'rgba(54, 162, 235, 0.5)',
                    borderColor: 'rgba(54, 162, 235, 1)',
                    borderWidth: 1
                }]
            },
            options: {
                scales: {
                    y: {
                        beginAtZero: true,
                        stepSize: 1
                    }
                }
            }
        });
    }
</script>

</body>
</html>
